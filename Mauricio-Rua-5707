<script type='text/javascript'>

var gptAds = gptAds || {};

googletag.cmd.push(function() {

  console.log('Prebid: googletag.cmd.push');

if (!(typeof(window.I10C) !== 'undefined' && window.I10C.Morph == 1)) {
gptAds['adoop']    = googletag.defineOutOfPageSlot('/4403/cr/sherdog/fightfinder', 'adoop').addService(googletag.pubads());
} else {
googletag.pubads().setTargeting('instartLogic', '1');
}
gptAds['ad728']    = googletag.defineSlot('/4403/cr/sherdog/fightfinder', [[728,90], [970,90]], 'ad728').addService(googletag.pubads()).setTargeting('pos', 'a');
gptAds['ad300']    = googletag.defineSlot('/4403/cr/sherdog/fightfinder', [[300,250], [300,600], [300,1050]], 'ad300').addService(googletag.pubads()).setTargeting('pos', 'a');
gptAds['ad600300'] = googletag.defineSlot('/4403/cr/sherdog/fightfinder', [[600,300]], 'ad600300').addService(googletag.pubads()).setTargeting('pos', 'a');
gptAds['ad600']    = googletag.defineSlot('/4403/cr/sherdog/fightfinder', [[600,250]], 'ad600').addService(googletag.pubads()).setTargeting('pos', 'b');
gptAds['ad320']    = googletag.defineSlot('/4403/cr/sherdog/fightfinder', [[300,50], [320,50], [320,100], [320,150]], 'ad320').addService(googletag.pubads()).setTargeting('pos', 'a');
gptAds['ad728ng']  = googletag.defineSlot('/4403/cr/sherdog/fightfinder', [[728,90]], 'ad728ng').addService(googletag.pubads()).setTargeting('pos', 'c');
gptAds['ad300ng']  = googletag.defineSlot('/4403/cr/sherdog/fightfinder', [[300,250]], 'ad300ng').addService(googletag.pubads()).setTargeting('pos', 'c');
gptAds['ad300st']    = googletag.defineSlot('/4403/cr/sherdog/fightfinder', [[300,250]], 'ad300st').addService(googletag.pubads()).setTargeting('pos', 'v');
googletag.pubads().setTargeting('ct', 'fightfinder').setTargeting('site', 'sherdog').setTargeting('fighter_id', '5707');
  googletag.pubads().enableSingleRequest();
//  googletag.pubads().disableInitialLoad();
  googletag.pubads().collapseEmptyDivs(true);
  googletag.enableServices();

  pbjs.que.push(function() {
    console.log('Prebid: pbjs.que.push');
    /* Set query string targeting on all GPT ad units. The logic for deciding 
       query strings is described in the section Configure AdServer Targeting. 
       Note that this function has to be called after all ad units on page are 
       defined. */
    pbjs.setTargetingForGPTAsync();
    console.log('Prebid: Are all bids available? : ' + pbjs.allBidsAvailable());
    console.log('Prebid: Targeting params:')
    console.log(pbjs.getAdserverTargeting());
    console.log('Prebid: Dumping all bid objects to console:');
    console.log(pbjs.getBidResponses());
  });
  
});


function refreshBids() {
  pbjs.que.push(function() {
    pbjs.requestBids({
      timeout: PREBID_TIMEOUT,
      bidsBackHandler: function() {
        pbjs.setTargetingForGPTAsync();
        googletag.pubads().refresh(); 
      }
    });
  });
}

